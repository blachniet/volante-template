{
  "name": "volante-template",
  "version": "0.0.1",
  "description": "Template for Volante backend project using mongo",
  "bin": {
    "volante-template": "src/index.js"
  },
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec npm start",
    "start": "node src/index.js",
    "build": "docker build -t localhost:5000/volante-template ."
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "volante": "^3.11.0",
    "volante-console": "^2.3.2",
    "volante-dashboard": "^3.2.0",
    "volante-express": "^4.0.0",
    "volante-mongo": "^6.3.1",
    "volante-swagger": "^3.0.0"
  }
}
