{
  "name": "volante-template",
  "version": "0.0.1",
  "description": "Template for Volante backend project using mongo",
  "bin": {
    "volante-template": "src/index.js"
  },
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec npm start",
    "start": "node src/index.js",
    "build": "docker build -t localhost:5000/volante-template ."
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "axios": "^0.27.0",
    "lodash": "^4.17.21",
    "volante": "^6.4.0",
    "volante-console": "^4.1.0",
    "volante-express": "^7.0.0",
    "volante-mongo": "^9.3.0",
    "volante-swagger": "^5.0.0"
  }
}
